    #s4-workspace
      #s4-bodyContainer<%= compassBootstrap ? ": .container" : "" %>
        .row
          .col-md-12
            img(src='images/ei_logo.png', class='pull-right', alt='Experts Inside')
        .row
          .col-md-12
            ul.nav.nav-pills
              li.active
                a(href='#') Experts Inside
              li
                a(href='#') Services
              li
                a(href='#') Products
              li
                a(href='#') Team
              li
                a(href='#') Contact
        .row
          .col-md-12
            .page-header
              img(src='http://placehold.it/1140x300', alt='Page Header')
        .row
          .col-md-12
            // CS: Start PlaceHolderMain Snippet
            // MS: <asp:ContentPlaceHolder ID="PlaceHolderMain" runat="server">
            block content
            // MS: <PublishingControls:EditModePanel runat="server">
